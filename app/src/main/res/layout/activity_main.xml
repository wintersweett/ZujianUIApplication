<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <com.example.zujianapplication.BallMoveView
        android:id="@+id/bmv"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="300dp"/>
    <com.example.zujianapplication.CoordinateView
        android:id="@+id/cl"
        app:layout_constraintTop_toBottomOf="@id/bmv"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.ClipView
        android:id="@+id/cv"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.WatchView
        android:id="@+id/wv"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.Line1View
        android:id="@+id/lv1"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.Line2View
        android:id="@+id/lv2"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.Rect1View
        android:id="@+id/rect1View"
