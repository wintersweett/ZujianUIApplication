        android:id="@+id/rect3V"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.ShaderView
        android:id="@+id/sv"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.GradientView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.RadialGradientView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.FiveChessView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.SweepGradientView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
   <com.example.zujianapplication.Sweep
       android:visibility="gone"
       android:layout_width="match_parent"
       android:layout_height="match_parent"/>
    <com.example.zujianapplication.PorterDuffXfermode
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.CirclePhotoView
       android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
