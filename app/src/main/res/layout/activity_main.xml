        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.FirstView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.CircleImageView
        android:layout_width="300dp"
        app:circle_border="10dp"
        android:visibility="gone"
        app:circle_border_color="@color/teal_200"
        android:layout_height="300dp"/>
    <com.example.zujianapplication.SizeViewGroup
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.CornerLayoutView
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="match_parent">
        <TextView
            android:text="我是中国人"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:text="我是中国人"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:text="我是中国人"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:text="我是中国人"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </com.example.zujianapplication.CornerLayoutView>
    <com.example.zujianapplication.FlowLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:text="1"
            android:gravity="center"
            android:layout_width="200dp"
            android:layout_height="300dp"/>
        <TextView
            android:text="2"
            android:gravity="center"
            android:layout_width="400dp"
            android:layout_height="500dp"/>
        <TextView
            android:text="3"
            android:gravity="center"
            android:layout_width="100dp"
            android:layout_height="30dp"/>
        <TextView
            android:text="4"
            android:gravity="center"
            android:layout_width="800dp"
            android:layout_height="30dp"/>
        <TextView
            android:text="5"
            android:gravity="center"
            android:layout_width="20dp"
            android:layout_height="80dp"/>
    </com.example.zujianapplication.FlowLayout>
    <com.example.zujianapplication.ShaderView
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <TextView
        android:id="@+id/tv"
        android:text="我是中国人中国人不骗中国人好不好嘛好不好嘛"
        android:textSize="20dp"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.zujianapplication.ImageViewOfMine
        android:id="@+id/iv"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>